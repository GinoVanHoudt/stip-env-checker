{
  "manifest_version": 3,
  "name": "STiP/CHiP env checker",
  "description": "Extension to change the color of the header based on the environment",
  "version": "1.0",
  "action": {
    "default_icon": "icon-512.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.seatankterminal.com/*", "http://localhost:4200/*"],
      "css": ["my-styles.css"],
      "js": ["content-script.js"]
    }
  ]
}
